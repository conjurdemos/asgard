# automatically generated by Conjur container config

user nobody;
error_log stderr debug;

events {
  worker_connections 1024;
}

http {
  <% upstreams.each do |name, target| %>
    upstream <%=name%> {
      server <%=target%>;
    }
  <% end %>

  <% servers.each do |server| %>
    include <%= server.conf_file %>;
  <% end %>
}
